{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Transform": "AWS::Serverless-2016-10-31",
    "Description": "Simple example Lambda-based website.",
    "Resources": {
        "PythonLambdaRuntime": {
            "Type" : "AWS::SSM::Parameter",
            "Properties" : {
                "DataType" : "text",
                "Description" : "",
                "Name" : "/dev/PythonLambdaRuntime",
                "Type" : "String",
