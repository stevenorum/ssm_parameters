        "CodeBuildImage": {
            "Type" : "AWS::SSM::Parameter",
            "Properties" : {
                "DataType" : "text",
                "Description" : "",
                "Name" : "/dev/CodeBuildImage",
                "Type" : "String",
                "Value" : "aws/codebuild/amazonlinux2-x86_64-standard:4.0"
            }
        }
    }
}
